import java.util.TreeSet;

public class Main {
	public static int cal(int a, int b,int s){
		if(a+b==s){
			return -1;
		}
		else if(a+b>s){
			return 0;
		}
		else{
			int alpha =  cal(a+b,b,s);
			int beta =  cal(a,a+b,s);
			
		}
		
	}
	public static void main(String[] args){
		int a = cal(1,2,5);
		if(a==-1)
			System.out.println("YES");
		else if(a==0)
			System.out.println("NO");
	}

}
