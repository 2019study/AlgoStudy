import java.util.TreeSet;

public class Main {
	public static int cal(int a, int b,int s){
		if(a+b==s){
			return -1;
		}
		else if(a+b>s){
			return 0;
		}
		return cal(a+b,b,s);
	}
	public static void main(String[] args){
		if(cal(1,2,3)==-1)
			System.out.println("YES");
		else
			System.out.println("NO");
	}

}
